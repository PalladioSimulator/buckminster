<rmap
	xmlns="http://www.eclipse.org/buckminster/RMap-1.0"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:bc="http://www.eclipse.org/buckminster/Common-1.0">

	<searchPath name="subclipse-core">
		<provider readerType="svn" componentType="eclipse.project" mutable="false" source="true">
			<uri format="http://subclipse.tigris.org/svn/subclipse/trunk/subclipse?moduleBeforeTag&amp;moduleAfterTag&amp;moduleAfterBranch#core"/>
		</provider>
	</searchPath>

	<searchPath name="subclipse-others">
		<provider readerType="svn" componentType="eclipse.project" mutable="false" source="true">
			<uri format="http://subclipse.tigris.org/svn/subclipse/trunk/subclipse?moduleBeforeTag&amp;moduleAfterTag&amp;moduleAfterBranch#{0}">
				<bc:propertyRef key="buckminster.component"/>
			</uri>
		</provider>
	</searchPath>

	<searchPath name="buckminster">
		<provider readerType="svn" componentType="eclipse.project" mutable="true" source="true">
			<uri format="svn://dev.eclipse.org/svnroot/tools/org.eclipse.buckminster/trunk/{0}?moduleAfterTag&amp;moduleAfterBranch">
				<bc:propertyRef key="buckminster.component" />
			</uri>
		</provider>
	</searchPath>

	<locator searchPathRef="subclipse-core" pattern="^org\.tigris\.subversion\.subclipse\.core$" />
	<locator searchPathRef="subclipse-others" pattern="^org\.tigris\.subversion\..*$" />
	<locator searchPathRef="buckminster" pattern="^org\.eclipse\.buckminster(\..*)?$" />
</rmap>