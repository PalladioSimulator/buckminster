<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
   <extension
         point="org.eclipse.buckminster.cmdline.commands">
		<command
			addhelpflags="true"
			class="org.eclipse.buckminster.manifest.commands.CreateManifest"
			name="createmanifest"
			status="NORMAL">
			<alias name="mkmf" />
		</command>
		<command
			addhelpflags="true"
			class="org.eclipse.buckminster.manifest.commands.DiffManifest"
			name="diffmanifest"
			status="NORMAL">
			<alias name="diffmf" />
		</command>
   </extension>

</plugin>
