<?xml version="1.0" encoding="UTF-8"?>
<bm:rmap
	xmlns:bm="http://www.eclipse.org/buckminster/RMap-1.0"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">

	<bm:site name="eng.tada.se">
		<bm:property key="p4.port" value="taby4:8992"/>
	</bm:site>

	<bm:searchPath name="frameworx">
		<bm:provider providerType="org.eclipse.team.cvs.core.cvsnature">
			<bm:uri format="1.0,:pserver:frameworx.tigris.org/cvs,frameworx/{0},{1}">
				<bm:replace pattern="com\.frameworx\.(\w+)" replacement="$1">
					<property key="buckminster.component.name"/>
				</bm:replace>
				<property key="buckminster.component.name"/>
			</bm:uri>
		</bm:provider>
	</bm:searchPath>

	<searchPath name="jrockit">
		<provider providerType="net.sourceforge.perforce.core.perforcenature">
			<uri format="1.0,{0},//depot/jrockit/dev/tools/{1}/current/eclipse/plugin,{2}">
				<property key="p4.port"/>
				<replace pattern="com\.jrockit\.(\w+)" replacement="$1">
					<property key="buckminster.component.name"/>
				</replace>
				<property key="buckminster.component.name"/>
			</uri>
		</provider>
	</searchPath>

	<searchPath name="maven">
		<provider providerType="org.eclipse.buckminster.team.urinature">
			<uri format="http://www.ibiblio.org/maven/{0}/jars/{1}-{2}.jar">
				<property key="buckminster.component.name"/>
				<replace pattern=".*\.(\w+)" replacement="$1">
					<property key="buckminster.component.target"/>
				</replace>
				<property key="buckminster.component.version"/>
			</uri>
		</provider>
	</searchPath>

	<locator searchPathRef="tigris" pattern="(se\.tada\.util\.sax)|(se\.tada\.util\.jpp)"/>
	<redirect href="http://buckminster.tigris.org/rmaps/apache.rmap" pattern="org\.apache\..*"/>
	<locator searchPathRef="p4" pattern="se\.tada\..*"/>
	<locator searchPathRef="maven" pattern=".*"/>
</bm:rmap>
