<plugin>

   <extension point="org.eclipse.emf.edit.itemProviderAdapterFactories">
      <factory
            uri="http://www.eclipse.org/buckminster/MavenProvider-1.0"
            class="org.eclipse.buckminster.rmap.maven.provider.MavenItemProviderAdapterFactory"
            supportedTypes=
              "org.eclipse.emf.edit.provider.IEditingDomainItemProvider
               org.eclipse.emf.edit.provider.IStructuredItemContentProvider
               org.eclipse.emf.edit.provider.ITreeItemContentProvider
               org.eclipse.emf.edit.provider.IItemLabelProvider
               org.eclipse.emf.edit.provider.IItemPropertySource"/>
   </extension>
   <extension
         point="org.eclipse.buckminster.rmap.edit.rmapProviders">
      <rmapProvider
            class="org.eclipse.buckminster.rmap.maven.provider.MavenProviderExtension">
      </rmapProvider>
   </extension>

</plugin>
