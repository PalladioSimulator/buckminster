<?xml version="1.0" encoding="UTF-8"?>
<feature
      id="org.eclipse.buckminster.maven.headless.feature"
      version="1.2.0.qualifier"
      label="%featureName"
      provider-name="%providerName">

   <description>
      %description
   </description>

   <copyright>
      %copyright
   </copyright>

   <license url="%licenseURL">
      %license
   </license>

   <requires>
      <import plugin="org.eclipse.buckminster.download" version="1.1.0" match="greaterOrEqual"/>
      <import plugin="org.eclipse.buckminster.runtime" version="1.1.0" match="greaterOrEqual"/>
      <import plugin="org.eclipse.buckminster.sax" version="1.0.0" match="greaterOrEqual"/>
      <import plugin="org.eclipse.buckminster.core" version="1.1.350" match="greaterOrEqual"/>
      <import plugin="org.eclipse.buckminster.osgi.filter" version="1.0.0" match="greaterOrEqual"/>
      <import plugin="org.eclipse.core.runtime" version="3.5.0" match="greaterOrEqual"/>
      <import plugin="org.eclipse.ecf" version="3.0.0" match="greaterOrEqual"/>
      <import plugin="org.eclipse.equinox.p2.core" version="1.0.100" match="greaterOrEqual"/>
   </requires>

   <plugin
         id="org.eclipse.buckminster.maven"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

</feature>
