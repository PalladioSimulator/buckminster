<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>
<plugin>

     <extension
         point="org.eclipse.buckminster.core.readerTypes">
      <readerType
            class="org.eclipse.buckminster.cvspkg.internal.CVSReaderType"
            id="cvs"/>
   </extension>
   	<extension point="org.eclipse.ant.core.antTasks">
		<antTask
			library="ant_tasks/ant_tasks.jar"
			name="buckminster.cvs.lastTimestamp"
			class="org.eclipse.buckminster.cvspkg.ant.LastTimestamp">
		</antTask>
   </extension>
    <extension
          point="org.eclipse.buckminster.core.forcedActivations">
       <forcedActivation
             class="org.eclipse.buckminster.cvspkg.CVSPlugin"
             pluginId="org.eclipse.buckminster.cvs"/>
    </extension>
</plugin>
