<plugin>

   <extension point="org.eclipse.emf.ecore.generated_package">
      <package
            uri="http://www.eclipse.org/buckminster/2009/aggregator"
            class="org.eclipse.buckminster.aggregator.AggregatorPackage"
            genModel="model/Aggregator.genmodel"/>
   </extension>

   <extension point="org.eclipse.emf.ecore.extension_parser">
      <parser
            type="build"
            class="org.eclipse.buckminster.aggregator.util.AggregatorResourceFactoryImpl"/>
   </extension>
   <extension
         point="org.eclipse.emf.ecore.protocol_parser">
      <parser
            class="org.eclipse.buckminster.aggregator.p2.util.MetadataRepositoryResourceFactoryImpl"
            protocolName="p2">
      </parser>
   </extension>

</plugin>
